{
  "name": "@tsrx-stack/source",
  "description": "ts-rest full-stack with fastify + slonik + postgres + react + tailwindcss in an nx monorepo",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "NODE_OPTIONS=\"--max-old-space-size=2048\" nx run-many --target=serve --projects=fastify-api,react-ui --parallel",
    "build": "nx run-many --target=build --all --parallel=false --output-style=static --verbose --exclude=\"@tsrx-stack/source\"",
    "build:clean": "nx run-many --target=build --all --parallel=false --output-style=static --verbose --exclude=\"@tsrx-stack/source\" --skip-nx-cache",
    "test": "NODE_OPTIONS=\"--max-old-space-size=2048\" nx run-many --all --target=test --exclude=\"@tsrx-stack/source\"",
    "test:watch": "NODE_OPTIONS=\"--max-old-space-size=2048\" nx run-many --all --target=test --watch --exclude=\"@tsrx-stack/source\"",
    "lint": "NODE_OPTIONS=\"--max-old-space-size=2048\" nx run-many --all --target=lint --exclude=\"*e2e\" --exclude=\"@tsrx-stack/source\"",
    "format:check": "nx format:check",
    "format:write": "nx format:write",
    "prettier": "prettier . --write",
    "docker:up": "docker compose up -d && exit 0",
    "docker:down": "docker compose down",
    "docker:postgres:up": "docker compose up -d postgres && exit 0",
    "docker:postgres:bash": "docker exec -it pg-tsr bash && exit 0",
    "kill:port": "bash -c 'pids=$(lsof -i :$0 | grep LISTEN | awk \"{print \\$2}\"); if [[ -z $pids ]]; then echo \"No process found running on port $0\"; else echo \"Killing: $pids\"; echo $pids | xargs kill; fi'",
    "kill:vscode-server": "ps uxa | grep .vscode-server | awk '{print $2}' | xargs kill",
    "psql:local": "psql -h localhost -U postgres -d postgres",
    "setup:database": "./scripts/bash/setup-db.sh",
    "psql": "./scripts/bash/psql.sh",
    "cli:pg:adhoc": "DOTENV_CONFIG_PATH=.env tsx --tsconfig packages/database/manager/tsconfig.json -r dotenv-expand/config packages/database/manager/src/lib/cli/cli-adhoc-query.ts",
    "cli:pg:schema": "DOTENV_CONFIG_PATH=.env tsx --tsconfig packages/database/manager/tsconfig.json -r dotenv-expand/config packages/database/manager/src/lib/cli/cli-schema.ts",
    "cli:pg:seed": "DOTENV_CONFIG_PATH=.env tsx --tsconfig packages/database/manager/tsconfig.json -r dotenv-expand/config packages/database/manager/src/lib/cli/cli-seed.ts",
    "cli:pg:drop": "DOTENV_CONFIG_PATH=.env tsx --tsconfig packages/database/manager/tsconfig.json -r dotenv-expand/config packages/database/manager/src/lib/cli/cli-drop.ts",
    "cli:pg:setup": "DOTENV_CONFIG_PATH=.env tsx --tsconfig packages/database/manager/tsconfig.json -r dotenv-expand/config packages/database/manager/src/lib/cli/cli-setup.ts"
  },
  "private": true,
